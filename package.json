{
  "name": "bw-backend",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a family-recipes-bw",
    "rollbackh": "heroku run knex migrate:rollback -a family-recipes-bw",
    "databaseh": "heroku pg:psql postgresql-triangular-99220 --app family-recipes-bw",
    "seedh": "heroku run knex seed:run -a family-recipes-bw",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "engines": {
        "node": "14.15.4",
        "npm": "6.14.11"
      },
  
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "=2.8.5",
    "cross-env": "=7.0.3",
    "dotenv": "^8.2.0",
    "express": "=4.17.1",
    "helmet": "=4.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "=0.21.17",
    "knex-cleaner": "=1.3.1",
    "pg": "=8.5.1"
  },
  "devDependencies": {
    "@types/jest": "=26.0.20",
    
    "eslint": "=7.20.0",
    "jest": "=26.6.3",
    "nodemon": "=2.0.7",
    "supertest": "=6.1.3"
  }
}
